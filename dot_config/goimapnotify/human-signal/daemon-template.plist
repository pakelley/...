<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
   "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.user.goimapnotify-human-signal</string>
    <key>ProgramArguments</key>
    <array>
      <string>SCRIPT_PATH_PLACEHOLDER</string>
      <string>--mailbox=human-signal</string>
    </array>
    <key>RunAtLoad</key>
    <true />
    <key>KeepAlive</key>
    <true />
    <key>StandardErrorPath</key>
    <string>/tmp/com.user.goimapnotify-human-signal.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/com.user.goimapnotify-human-signal.out</string>
  </dict>
</plist>
